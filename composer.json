{
    "name": "amendozaaguiar/lates",
    "description": "Paquete de traducciones en español para Laravel 9, 10. Este paquete te agrega a tu proyecto los ficheros de traducciones en español que Laravel 9, 10 trae por defecto.",
    "keywords": [
        "laravel",
        "español",
        "traduccion",
        "spanish",
        "translation"
    ],
    "homepage": "https://github.com/amendozaaguiar/lates",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Amendozaaguiar\\Lates\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anderson Mendoza",
            "homepage": "https://amendozaaguiar.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": ">=5.4.0",
        "illuminate/console": ">=5.4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amendozaaguiar\\Lates\\LatesServiceProvider"
            ]
        }
    }
}
